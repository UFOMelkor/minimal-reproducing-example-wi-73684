{
  "name": "minimal-reproducible-example-web",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5"
  },
  "dependencies": {},
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "cacheDirectory": "build/jest-cache",
    "coverageDirectory": "build/jest-coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "tabWidth": 4,
    "trailingComma": "all"
  }
}
